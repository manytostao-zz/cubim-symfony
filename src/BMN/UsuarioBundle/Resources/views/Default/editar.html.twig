{% extends '::frontend2.html.twig' %}
{% block title %}
    <title>CUBiM | Registro</title>
{% endblock %}
{% block page_title %}
    <h3 class="page-title">
        Registro
    </h3>
{% endblock %}
{% block page_style %}

    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/clockface/css/clockface.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/bootstrap-datepicker/css/datepicker3.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/bootstrap-timepicker/css/bootstrap-timepicker.min.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/bootstrap-colorpicker/css/colorpicker.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/bootstrap-datetimepicker/css/datetimepicker.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('metronic/assets/global/plugins/select2/select2.css') }}" />
    <link href="{{ asset('metronic/assets/global/plugins/gritter/css/jquery.gritter.css') }}" rel="stylesheet"
          type="text/css"/>
{% endblock %}
{% block page_bar %}
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-users"></i>
            </li>
            <li>
                <i class="fa fa-angle-right"></i>
                <a href="{{ path('usuario_lista', {'modulo':'usuario'}) }}">Listado de Usuarios</a>
            </li>
            {% if id is defined %}
                <li>
                    <i class="fa fa-angle-right"></i>
                    <a href="{{ path('usuario_editar', {'modulo':'usuario', 'id':id}) }}"
                       title="Editar Usuario">Editar Usuario</a>
                </li>
            {% else %}
                <li>
                    <i class="fa fa-angle-right"></i>
                    <a href="{{ path('usuario_adicionar', {'modulo':'usuario'}) }}"
                       title="Adicionar Usuario">Adicionar Usuario</a>
                </li>
            {% endif %}
        </ul>
        <div class="page-toolbar">
            <div class="btn-group pull-right">
                <a data-close-others="true" class="btn btn-fit-height btn-primary dropdown-toggle"
                   href="#" onclick="Usuario.noCIConfirm()">
                    <i class="fa fa-save"></i> &nbsp;Salvar
                </a>
            </div>
        </div>
    </div>
{% endblock %}
{% block page_content %}
    {{ form_start(formulario) }}
    <div id="errores" class="row">
        <div class="col-md-12">
            {% if (form_errors(formulario)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>{{ form_errors(formulario) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.nombres)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.nombres) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.apellidos)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.apellidos) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.fechaIns)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.fechaIns) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.carnetId)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.carnetId) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.carnetBib)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.carnetBib) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.email)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.email) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.telefono)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.telefono) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.experiencia)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.experiencia) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.tipoPro)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.tipoPro) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.especialidad)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.especialidad) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.profesion)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.profesion) }}
                </div>
            {% endif %}
            {% if (form_errors(formulario.estudiante)) %}
                <div class="alert alert-danger">
                    <button class="close" aria-hidden="true" data-dismiss="alert" type="button"></button>
                    <i class="fa-lg fa fa-warning pull-left"></i>
                    {{ form_errors(formulario.estudiante) }}
                </div>
            {% endif %}
            <div class="note note-success" style="padding-bottom: 5px; padding-top: 10px">
                <h4 style="margin: 0" class="col-lg-5">
                    <span class="col-lg-4"
                          style="padding: 0">{{ form_widget(formulario.tipoUsua,{ 'attr': {'class':'form-control' }}
                        ) }}</span>
                    <span class="col-lg-1" style="padding: 10px">#</span>
                    <span class="col-lg-3"
                          style="padding: 0">{{ form_widget(formulario.carnetBib,{ 'attr': {'class':'form-control col-lg-3' }}
                        ) }}</span>
                </h4>

                <div class="pull-right">
                    <button class="btn blue" title="Últimos números asignados"
                            onclick="DSI.notiLatestNumbers({{ temporales[0][1] }}, {{ potenciales[0][1] }})" type="button"><i class="fa fa-info"></i></button>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="portlet box blue-hoki tabbable">
                <div class="portlet-title">
                    <div class="caption">
                        <em class="fa fa-user"><sup class="fa fa-list"></sup></em>&nbsp;Detalles de Usuario
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="tabbable portlet-tabs">
                        <ul class="nav nav-tabs">
                            <li>
                                <a data-toggle="tab" href="#portlet_tab_3">
                                    Categor&iacute;as </a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#portlet_tab_2">
                                    Datos Profesionales </a>
                            </li>
                            <li class="active">
                                <a data-toggle="tab" href="#portlet_tab_1">
                                    Datos Personales </a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div id="portlet_tab_3" class="tab-pane">
                                <div class="table-scrollable">
                                    <table role="grid" id="categorias"
                                           class="table table-hover">
                                        <tbody>
                                        <tr>
                                            <td>
                                                <strong>Docente</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.categDoc,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Investigativa</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.categInv,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Cient&iacute;fica</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.categCien,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Ocupacional</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.categOcup,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div id="portlet_tab_2" class="tab-pane">
                                <div class="table-scrollable">
                                    <table role="grid" id="profesionales"
                                           class="table table-hover">
                                        <tbody>
                                        <tr role="row">
                                            <td>
                                                <strong>Estudiante</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.estudiante,{
                                                        'attr': {'class':'form-control' , 'onclick':
                                                            'Usuario.switchEditSelectsByCheck()' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr role="row">
                                            <td>
                                                <strong><span
                                                            id="tipoPro">{% if formulario.estudiante.vars.value == false %}Tipo de Profesional{% else %}Carrera en Salud{% endif %}</span>
                                                </strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.tipoPro,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr id="especialidad"
                                            {% if formulario.estudiante.vars.value == true %}style="display: none"{% endif %}>
                                            <td>
                                                <strong>Especialidad</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.especialidad,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong><span
                                                            id="profesion"> {% if formulario.estudiante.vars.value == false %}Profesi&oacute;n {% else %}Carrera fuera de Salud {% endif %}</span></strong>

                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.profesion,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr id="cargo"
                                            {% if formulario.estudiante.vars.value == true %}style="display: none"{% endif %}>
                                            <td>
                                                <strong>Cargo</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.cargo,{ 'attr': {'class':'form-control input-large' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Instituci&oacute;n</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.institucion,{ 'attr': {'class':'form-control input-xlarge' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr id="dedicacion"
                                            {% if formulario.estudiante.vars.value == true %}style="display: none"{% endif %}>
                                            <td>
                                                <strong>Dedicaci&oacute;n</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.dedicacion,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        <tr id="experiencia"
                                            {% if formulario.estudiante.vars.value == true %}style="display: none"{% endif %}>
                                            <td>
                                                <strong>Experiencia (en a&ntilde;os)</strong>
                                            </td>
                                            <td>
                                                <div class="pull-right"
                                                     style="text-align: right">{{ form_widget(formulario.experiencia,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}
                                                </div>
                                            </td>
                                        </tr>
                                        {% if usuario is defined and usuario.tipoUsua == 'Potencial' %}
                                            <tr id="atendidoPor">
                                                <td>
                                                    <strong>Atendido por</strong>
                                                </td>
                                                <td>
                                                    <p class="pull-right"
                                                       style="text-align: right">{{ form_widget(formulario.atendidoPor,{ 'attr': {'class':'form-control input-medium' }}
                                                        ) }}</p>
                                                </td>
                                            </tr>
                                        {% else %}
                                            {% do formulario.atendidoPor.setRendered %}
                                        {% endif %}
                                        <tr id="temaInv">
                                            <td>
                                                <strong>Tema de Investigaci&oacute;n</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.temaInv,{ 'attr': {'class':'form-control input-large' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div id="portlet_tab_1" class="tab-pane active">
                                <div class="table-scrollable">
                                    <table role="grid" id="personales"
                                           class="table table-hover">
                                        <tbody>
                                        <tr role="row">
                                            <td>
                                                <strong>Nombre</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.nombres,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr role="row">
                                            <td>
                                                <strong>Apellidos</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.apellidos,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr role="row">
                                            <td>
                                                <strong>Carnet de Identidad</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.carnetId,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr role="row">
                                            <td>
                                                <strong>Pa&iacute;s</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.pais,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Correo Electr&oacute;nico</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.email,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Tel&eacute;fono</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.telefono,{ 'attr': {'class':'form-control input-medium' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Fecha de Inscripci&oacute;n</strong>
                                            </td>
                                            <td>
                                                {{ form_widget(formulario.fechaIns, {'attr':{'class':'form-control inline input-medium date-picker pull-right', 'data-date-format':'dd/mm/yyyy', 'data-toggle':'masked', 'data-inputmask': "'mask': '99/99/9999'", 'style':'width:100%' }}
                                                ) }}
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <strong>Observaciones</strong>
                                            </td>
                                            <td>
                                                <p class="pull-right"
                                                   style="text-align: right">{{ form_widget(formulario.observaciones,{ 'attr': {'class':'form-control input-large' }}
                                                    ) }}</p>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {% do formulario.fechaIns.setRendered %}
    {% do formulario.fechaInsDesde.setRendered %}
    {% do formulario.fechaInsHasta.setRendered %}
    {% do formulario.inside.setRendered %}
    {% do formulario.currentlyInNav.setRendered %}
    {% do formulario.currentlyInLect.setRendered %}
    {% do formulario.inactivo.setRendered %}
    {{ form_end(formulario) }}
    <div class="row hidden">
        <div class="col-md-12">
            <div class="portlet yellow box">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-cogs"></i>Notific8 Notification Demo
                    </div>
                    <div class="tools">
                        <a href="javascript:" class="collapse">
                        </a>
                        <a href="#portlet-config" data-toggle="modal" class="config">
                        </a>
                        <a href="javascript:" class="reload">
                        </a>
                        <a href="javascript:" class="remove">
                        </a>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="note note-success">
                        <h4 class="block">jquery.notific8</h4>

                        <p>
                            jquery.notific8 is a notification plug-in that was inspired by the notification style
                            introduced in Windows 8. For more info Check out <a
                                    href="https://github.com/willsteinmetz/jquery-notific8" target="_blank">
                                the official github respository </a>
                        </p>
                    </div>
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Notification text:</label>

                            <div class="col-md-5">
                                <input id="notific8_text" type="text" class="form-control"
                                       value="El usuario {{ formulario.nombres.vars.value }} {{ formulario.apellidos.vars.value }}
                                       se encuentra actualmente en la biblioteca" placeholder="enter a text ..."/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Heading(optional):</label>

                            <div class="col-md-5">
                                <input id="notific8_heading" type="text" class="form-control"
                                       value="Notificación de CUBiM"
                                       placeholder="enter a heading ..."/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Sticky ?</label>

                            <div class="col-md-5">
                                <label class="checkbox">
                                    <input type="checkbox" id="notific8_sticky" value="1">
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Life:</label>

                            <div class="col-md-5">
                                <select id="notific8_life" class="form-control input-medium">
                                    <option value="1000">1 second</option>
                                    <option value="5000" selected="selected">5 seconds</option>
                                    <option value="10000">10 seconds (default)</option>
                                    <option value="25000">25 seconds</option>
                                    <option value="60000">60 seconds</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Style:</label>

                            <div class="col-md-5">
                                <select id="notific8_theme" class="form-control input-medium">
                                    <option value="teal" selected="selected">teal (default)</option>
                                    <option value="amethyst">amethyst</option>
                                    <option value="ruby">ruby</option>
                                    <option value="tangerine">tangerine</option>
                                    <option value="lemon">lemon</option>
                                    <option value="lime">lime</option>
                                    <option value="ebony">ebony</option>
                                    <option value="smoke">smoke</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title">Position:</label>

                            <div class="col-md-5">
                                <select id="notific8_pos_hor" class="form-control input-small input-inline">
                                    <option value="top">top (default)</option>
                                    <option value="bottom">bottom</option>
                                </select>
                                <select id="notific8_pos_ver" class="form-control input-small input-inline">
                                    <option value="right">right (default)</option>
                                    <option value="left">left</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label" for="title"></label>

                            <div class="col-md-5">
                                <a href="javascript:" class="btn green-meadow btn-lg" id="notific8_show">
                                    Show Notification! </a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div aria-hidden="false" role="dialog" tabindex="-1" id="save" class="modal fade bs-modal-sm in"
         style="display: none;">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button"></button>
                    <h4 class="modal-title"><i class="fa fa-warning"></i>&nbsp;Confirmación</h4>
                </div>
                <div class="modal-body">
                    ¿Está seguro que desea salvar este usuario sin Carnet de Identidad?
                </div>
                <div class="modal-footer">
                    <a id="eliminar" onclick="document.forms[0].submit()" class="btn blue">Si</a>
                    <button data-dismiss="modal" class="btn default" type="button">No</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
