{% extends '::frontend2.html.twig' %}
{% block title %}
    <title>CUBiM | Registro</title>
{% endblock %}
{% block page_title %}
    <h3 class="page-title">
        Usuario no encontrado <small>P&aacute;gina de error</small>
    </h3>
{% endblock %}
{% block page_style %}
    <!-- BEGIN PAGE LEVEL STYLES -->
    <link href="{{ asset('metronic/assets/admin/pages/css/error.css') }}" rel="stylesheet" type="text/css"/>
    <!-- END PAGE LEVEL STYLES -->
{% endblock %}
{% block page_bar %}
    <!-- BEGIN PAGE HEADER-->
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-users"></i>
            </li>
            <li>
                <i class="fa fa-angle-right"></i>
                <a href="{{ path('usuario_lista', {'modulo':'usuario'}) }}">Listado de Usuarios</a>
            </li>
            <li>
                <i class="fa fa-angle-right"></i>
                <a href="#">Usuario no encontrado</a>
            </li>
        </ul>
        <div data-placement="top" class="pull-right tooltips btn btn-fit-height grey-salt" id="dashboard-report-range">
            <i class="fa fa-calendar"></i>&nbsp; <span
                    class="thin uppercase visible-lg-inline-block">{{ 'today'|date('d/m/Y') }}</span>&nbsp;
        </div>
    </div>
    <!-- END PAGE HEADER-->
{% endblock %}
{% block page_content %}

    <!-- BEGIN PAGE CONTENT-->
    <div class="row">
        <div class="col-md-12 page-404" style="vertical-align: middle">
            <div class="number" style="display: inline-table;">
                404
            </div>
            <div class="details" style="display: inline-table;">
                <h3>Â¡Ups! Parece que est&aacute;s perdid@.</h3>
                <p>
                    No podemos encontrar el usuario que buscas.<br/>
                    <a href="{{ path('usuario_lista', {'modulo':'usuario'}) }}">
                        Regresa al listado </a>
                    o regresa atr&aacute;s en el historial <i class="fa fa-arrow-left"></i>.<br/>
                    Este error puede estar provocado porque accediste desde <br/>
                    las trazas, al detalle de un usuario que ya ha sido eliminado <br/>
                    o porque introdujiste incorrectamente una URL.
                </p>
            </div>
        </div>
    </div>
{% endblock %}
{% block scripts %}
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="{{ asset('metronic/assets/global/plugins/gritter/js/jquery.gritter.js') }}" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <script src="{{ asset('metronic/assets/global/scripts/metronic.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/layout.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/quick-sidebar.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/demo.js') }}" type="text/javascript"></script>
    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            QuickSidebar.init(); // init quick sidebar
            Demo.init(); // init demo features
        });
    </script>
    <!-- END JAVASCRIPTS -->
{% endblock %}