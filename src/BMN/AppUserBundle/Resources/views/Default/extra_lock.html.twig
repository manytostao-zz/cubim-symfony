                 class="page-lock-img">
        {% endif %}
        <div class="page-lock-info">
            <h1>{{ app.user.nombre }}</h1>
            {% for role in roles %}
            <span class="email">
			{{ role.name }} </span>
            {% endfor %}
			<span class="locked">
			Bloqueado </span>

            <form class="form-inline" action={{ path('appuser_login_check') }}>
                <div class="input-group input-medium">
                    <input type="password" class="form-control" placeholder="Contraseña">
					<span class="input-group-btn">
					<button type="submit" class="btn blue icn-only"><i class="m-icon-swapright m-icon-white"></i>
                    </button>
					</span>
                </div>
                <!-- /input-group -->
                <div class="relogin">
                    <a href="{{ path('appuser_logout') }}">
                        ¿No eres {{ app.user.nombre }} ? </a>
                </div>
            </form>
        </div>
    </div>
    <div class="page-footer-custom">
