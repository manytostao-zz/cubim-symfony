<div class="content" style="padding: 0">
    <div class="col-md-12">
        <div class="portlet solid grey-cascade" style="margin-top: 100px">
            <!-- BEGIN LOGO -->
            <div class="portlet-title box">
                <div class="logo" style="margin: 0; padding: 0">
