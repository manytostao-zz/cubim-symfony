{% extends '::frontend2.html.twig' %}
{% block title %}
    <title>CUBiM | Panel Resumen</title>
{% endblock %}
{% block page_style %}
    <link href="{{ asset('metronic/assets/global/plugins/gritter/css/jquery.gritter.css') }}" rel="stylesheet"
          type="text/css"/>
    <link href="{{ asset('metronic/assets/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css') }}"
          rel="stylesheet" type="text/css"/>
    <link href="{{ asset('metronic/assets/global/plugins/fullcalendar/fullcalendar.css') }}" rel="stylesheet"
          type="text/css"/>
    <link href="{{ asset('metronic/assets/global/plugins/qtip/css/jquery.qtip.css') }}" rel="stylesheet"
          type="text/css"/>
{% endblock %}
{% block page_title %}
    <h3 class="page-title">
        Panel Resumen
    </h3>
{% endblock %}
{% block page_bar %}
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
            </li>
            <li>
                <i class="fa fa-angle-right"></i>
                <a href="{{ path('otros_homepage') }}">Informaci&oacute;n estad&iacute;stica dependiente</a>
            </li>
        </ul>
        <div class="page-toolbar">
            <div id="dashboard-report-range" class="pull-right tooltips btn btn-fit-height grey-salt"
                 data-placement="top" data-original-title="Cambiar rango de fecha del Panel Resumen">
                <i class="fa fa-calendar"></i>&nbsp; <span class="thin uppercase visible-lg-inline-block"></span>&nbsp;
                <i class="fa fa-angle-down"></i>
            </div>
        </div>
    </div>
{% endblock %}
{% block page_content %}
    <div id="update">
        <div class="row">
            <!-- BEGIN BASIC CHART PORTLET-->
            <div class="col-md-12 col-sm-12">
                <div class="portlet box blue-hoki">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-bar-chart-o"></i>Usuarios por servicio
                        </div>
                        <div class="tools">
                            <a href="javascript:" class="collapse">
                            </a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div id="chart_1_1_legendPlaceholder">
                        </div>
                        <div id="chart_1_1" class="chart">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-7 col-sm-7">
                <div class="portlet box blue-hoki">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-bar-chart-o"></i>Usuarios por clasificaci&oacute;n
                        </div>
                        <div class="tools">
                            <a href="javascript:" class="collapse">
                            </a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <h4 id="total_users"></h4>

                        <div id="pie_chart_3" class="chart">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-5 col-sm-5">
                <!-- BEGIN PORTLET-->
                <div class="portlet paddingless">
                    <div class="portlet-title line">
                        <div class="caption">
                            <i class="fa fa-exclamation-circle"></i>Datos de inter&eacute;s
                        </div>
                        <div class="tools">
                            <a class="collapse" href="">
                            </a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <!--BEGIN TABS-->
                        <div class="tabbable tabbable-custom">
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a data-toggle="tab" href="#tab_1_3">
                                        Usuarios que m&aacute;s nos visitan </a>
                                </li>
                                <li>
                                    <a data-toggle="tab" href="#tab_1_2">
                                        De nuestros servicios </a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <div id="tab_1_3" class="tab-pane active">
                                    <div id="MAS" data-handle-color="#a1b2bd" data-rail-color="yellow"
                                         style="height: 245px; overflow: hidden; width: auto;" class="scroller">
                                    </div>
                                </div>
                                <div id="tab_1_2" class="tab-pane">
                                    <div data-handle-color="#a1b2bd" data-rail-color="yellow"
                                         style="height: 245px; overflow: hidden; width: auto;" class="scroller">
                                        <ul id="serv" class="feeds"></ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--END TABS-->
                    </div>
                </div>
                <!-- END PORTLET-->
            </div>
        </div>
    </div>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
            </li>
            <li>
                <i class="fa fa-angle-right"></i>
                <a href="{{ path('otros_homepage') }}">Informaci&oacute;n estad&iacute;stica independiente</a>
            </li>
        </ul>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-7 col-sm-7">
                <!-- BEGIN PORTLET-->
                <div class="portlet box blue-hoki calendar">
                    <div class="portlet-title">
                        <div class="tools">
                            <a href="javascript:" class="collapse">
                            </a>
                        </div>
                    </div>
                    <div class="portlet-body light-grey">
                        <div id="calendar">
                        </div>
                    </div>
                </div>
                <!-- END PORTLET-->
            </div>
            <div class="col-md-5">
                <div class="col-md-12">
                    <div class="dashboard-stat blue-madison" style="background-color: #edde34">
                        <div class="visual">
                            <i class="fa fa-users"></i>
                        </div>
                        <div class="details">
                            <div class="number">
                                {{ temporales[1] }}
                            </div>
                            <div class="desc">
                                Temporales
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="dashboard-stat blue-madison" style="background-color: #5bb136">
                        <div class="visual">
                            <i class="fa fa-users"></i>
                        </div>
                        <div class="details">
                            <div class="number">
                                {{ potenciales[1] }}
                            </div>
                            <div class="desc">
                                Potenciales
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="dashboard-stat green-haze">
                        <div class="visual">
                            <i class="fa fa-star-half-o"></i>
                        </div>
                        <div class="details">
                            <div class="number">
                                {{ masters[1] }}
                            </div>
                            <div class="desc">
                                M&aacute;steres
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="dashboard-stat blue-madison">
                        <div class="visual">
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="details">
                            <div class="number">
                                {{ doctores[1] }}
                            </div>
                            <div class="desc">
                                Doctores
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN INTERACTIVE CHART PORTLET-->
            <div class="portlet box blue-hoki">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-bar-chart-o"></i>Flujo de usuarios por meses
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse">
                        </a>
                        <a href="javascript:;" class="remove">
                        </a>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="chart_2" class="chart" style="height: 500px">
                    </div>
                </div>
            </div>
            <!-- END INTERACTIVE CHART PORTLET-->
        </div>
    </div>
{% endblock %}
{% block scripts %}

    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.resize.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.pie.min.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.stack.min.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.crosshair.min.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.orderBars.js') }}"></script>
    <script src="{{ asset('metronic/assets/global/plugins/flot/jquery.flot.categories.js') }}"
            type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <script src="{{ asset('metronic/assets/global/plugins/bootstrap-daterangepicker/moment.min.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/global/scripts/metronic.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/layout.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/quick-sidebar.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/layout/scripts/demo.js') }}" type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/global/plugins/gritter/js/jquery.gritter.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('metronic/assets/admin/pages/scripts/index.js') }}" type="text/javascript"></script>
    <script type="text/javascript"
            src="{{ asset('metronic/assets/global/plugins/fullcalendar/fullcalendar.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('metronic/assets/global/plugins/fullcalendar/lang-all.js') }}"></script>
    <script type="text/javascript" src="{{ asset('metronic/assets/global/plugins/qtip/js/jquery.qtip.js') }}"></script>
    <script type="text/javascript" src="{{ asset('metronic/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('bundles/otros/js/otros.js') }}"></script>
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/otros/js/otros.js') }}"></script>
    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Metronic.initSlimScroll('.scroller');
            Layout.init(); // init current layout
            QuickSidebar.init(); // init quick sidebar
            Demo.init(); // init demo features
            Index.init();
            Otros.initDashboardDaterangeLocal();
            Otros.apply();
            Otros.initCalendar();
            Otros.initInteractiveChart();
        });
    </script>
{% endblock %}